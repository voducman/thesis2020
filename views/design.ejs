<!doctype html>
<html lang="en">

<head>
    <% include metadata %>

    <link rel="stylesheet" href="static/css/design.css">
    <script src="/static/context-menu/context-menu.js"></script>
    <link rel="stylesheet" href="static/context-menu/context-menu.css"> 
</head>

<body>
    <div class="wrapper" id="main-view">
        <% include sidebar %>
        <div class="main-panel">
            <% include header %>
            <div class="content" style="padding-bottom: 0;">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-md-3 col-md-offset-9 text-right" style="margin-bottom: -2%">
                            <button type="button" rel="tooltip" class="btn btn-instagram btn-round"
                                data-original-title="" title="Create New" id="create-design">
                                <i class="material-icons">note_add</i>
                                Create New
                            </button>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12">
                            <div class="card" style="background-color: #e6e7e7;">
                                <div class="card-content">
                                    <div class="table-responsive">
                                        <table class="table" style="font-size: small;">
                                            <thead style="background-color: #d8d8d8;font-weight: 900!important;">
                                                <tr>
                                                    <th class="text-center">#</th>
                                                    <th class="text-center">Name</th>
                                                    <th class="text-center">Create Time</th>
                                                    <th class="text-center">Last Modified</th>
                                                    <th class="text-center">Complied</th>
                                                    <th class="text-center">Description</th>
                                                    <th class="text-center">Run</th>
                                                    <th class="text-center">Design</th>
                                                </tr>
                                            </thead>
                                            <tbody id="render-table">


                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <% include footer %>
            </div>
        </div>

        <% include addNewDesign %>
</body>
<% include scripts %>

<script type="text/javascript" src="/static/javascripts/design.bundle.js"></script>

</html>


